<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".leaderboard_page">

    <TextView
        android:id="@+id/textViewTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/leaderboard_page"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- Game Set 2 Leaderboard -->

    <TextView
        android:id="@+id/textViewGameSet1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/game_set_1"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="104dp" />

    <TextView
        android:id="@+id/textViewLeaderboard1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1. null: 0"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="137dp" />

    <TextView
        android:id="@+id/textViewLeaderboard2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2. null: 0"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="173dp" />

    <TextView
        android:id="@+id/textViewLeaderboard3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3. null: 0"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="208dp" />

    <TextView
        android:id="@+id/textViewGameSet2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Game Set 2"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="249dp" />

    <TextView
        android:id="@+id/textViewLeaderboard4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1. null: 0"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="288dp" />

    <TextView
        android:id="@+id/textViewLeaderboard5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2. null: 0"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="315dp" />

    <TextView
        android:id="@+id/textViewLeaderboard6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3. null: 0"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="341dp" />


</androidx.constraintlayout.widget.ConstraintLayout>